{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src",
    "test": "jest --verbose",
    "coverage": "jest --verbose --coverage",
    "test:ci": "jest --ci --coverage --silent --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "npm run build-storybook && storycap --serverCmd \"npx http-server storybook-static -p 6006\" http://0.0.0.0:6006 --serverTimeout 600000",
    "reg-suit": "reg-suit run",
    "translate-interface": "i18next-resources-for-ts interface -i ./src/translation/locales -o ./src/translation/@types/resources.d.ts",
    "translate-parse": "i18next 'src/**/*.{tsx,ts}' && npm run translate-interface",
    "knip": "npx knip"
  },
  "dependencies": {
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@vitejs/plugin-react": "4.2.1",
    "classnames": "^2.5.1",
    "js-cookie": "^3.0.5",
    "msw": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.23.1",
    "vite": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.2.2",
    "msw-storybook-addon": "^1.10.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "puppeteer": "^22.3.0",
    "reg-notify-gitlab-plugin": "^0.14.3",
    "reg-publish-s3-plugin": "^0.14.3",
    "reg-simple-keygen-plugin": "^0.14.2",
    "reg-suit": "^0.14.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "5.2",
    "undici": "^5.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
